<h1>Sell a ticket</h1>

<%= form_for @ticket, :html => { :multipart => true } do |f| %>

	<%= f.label :title %>:
	<%= f.text_field :title %><br />

	<%= f.label :description %>:
	<%= f.text_field :title %><br />

	<%= f.label :price %>:
	<%= f.text_field :price %><br />

	<%= f.file_field :photo %><br />

	<br />

	<%= f.submit %>

<% end %>